{
	"plugins": [
		"prettier-plugin-astro"
	],
	"trailingComma": "none",
	"useTabs": true,
	"tabWidth": 2,
	"semi": true,
	"singleQuote": false,
	"bracketSpacing": true,
	"arrowParens": "always",
	"printWidth": 80,
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		}
	]
}